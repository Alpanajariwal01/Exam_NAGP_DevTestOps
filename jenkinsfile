pipeline{
    agent any
    environment{
        notifyEmail = "alpana@nagarro.com"
    }
    triggers {
        cron('H 06 * * *')
    }
    stages{
        stage("Code Checkout"){
            steps{
                bat "echo checkout"
            }
        }
        stage("Code test"){
            steps{
                bat "echo test"
            }
        }
        stage("Code deploeyd"){
            steps{
                bat "echo deployed"
            }
        }
    }
    post{
        success{
            bat "echo success"
        }
    }
}    
